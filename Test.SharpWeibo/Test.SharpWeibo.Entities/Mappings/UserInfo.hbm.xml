<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Test.SharpWeibo.Entities" namespace="Test.SharpWeibo.Entities">
  <class name="UserInfo" table="WEBO_DATA_USERINFO" dynamic-update="true">
    <id name="ID" column="ID">
      <generator class="increment"></generator>
    </id>
    <property name="UserID" column="UserID"/>
    <property name="UserName" column="UserName"/>
    <property name="Password" column="Password"/>
    <property name="NeckName" column="NeckName"/>
    <property name="Gender" column="Gender"/>
    <property name="Email" column="Email"/>
    <property name="CreateDate" column="CreateDate"/>
    <property name="LastActiveDate" column="LastActiveDate"/>
  </class>
</hibernate-mapping>